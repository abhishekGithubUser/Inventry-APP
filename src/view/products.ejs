            

            <% if (locals.lastVisit){%>
            <div class="alert alert-primary mt-5" role="alert">
             Your last visite: <%= locals.lastVisit %>
            </div>
            <%}%>

         
        <!-- Table start -->
        <div class="bg-Secondary">
          <h1> Products </h1>
          <table class="table ">
            <thead>
              <tr>
                <th scope="col">ID</th>
                <th scope="col">name</th>
                <th scope="col">Discription</th>
                <th scope="col">Price</th>
                <th scope="col">Image</th>
                <th scope="col">Actions</th>
              </tr>
            </thead>
            <tbody>
              <% products.forEach( (product) => { %>
              <tr>
                <th scope="row"><%= product.id %></th>
                <td><%= product.name %></td>
                <td><%= product.desc %></td>
                <td><%= product.price %>$</td>
                <td> <img src="<%= product.imgUrl %>" width="80px" alt="Photo"> </td>
                <td> <a type="button" class="btn btn-warning mt-5" href="/update/<%= product.id %>">Update</a> 
                  <button class="btn btn-danger mt-5" onclick="deleteProduct('<%= product.id %>')">Delete</button>
                  <!-- <a type="button" class="btn btn-danger mt-5" href="/delete-product/<%= product.id %>">delete</a> </td> -->
                </tr>

                <%  }) %>
                
              
            </tbody>
          </table>
        </div>
       
        <!-- Table end -->
    